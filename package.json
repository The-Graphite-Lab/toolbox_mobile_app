{
  "name": "mobile-app-webpages",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prebuild": "node scripts/inject-version.js",
    "dev": "node scripts/inject-version.js && next dev",
    "build": "next build",
    "postbuild": "node scripts/inject-version.js",
    "start": "next start",
    "lint": "next lint",
    "cap:sync": "cap sync",
